{
  "scripts": {
    "dev": "NODE_ENV=development PORT=3001 node server.js",
    "build": "npm run clean && npm run build-server && npm run build-client",
    "start": "PORT=3000 NODE_ENV=production node server.js",
    "storybook": "start-storybook -s ./static -p 6006",
    "build-storybook": "build-storybook",
    "clean": "rm -rf build",
    "build-client": "next build && cp -R .next build/",
    "build-server": "node -v ",
    "git-pull": "git --version",
    "pm2-delete": "pm2 delete all",
    "pm2-status": "pm2 status",
    "pm2-logs": "pm2 logs",
    "pm2-run": "NODE_ENV=production pm2 start -i 4 server.js --env production --name pubhouse",
    "pm2-rerun": "pm2 restart pubhouse",
    "deploy": "npm install && npm run build && npm run pm2-run",
    "redeploy": "npm run git-pull && npm install && npm run build && yarn run pm2-rerun",
    "restart": "npm run pm2-delete && npm run deploy"
  },
  "dependencies": {
    "apicache": "^0.8.4",
    "axios": "^0.17.1",
    "classnames": "^2.2.5",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "express-http-proxy": "^1.0.4",
    "isomorphic-cookie": "^1.2.3",
    "isomorphic-style-loader": "^2.0.0",
    "lodash": "^4.17.4",
    "logatim": "^0.9.0",
    "next": "latest",
    "next-redux-wrapper": "^1.3.4",
    "nprogress": "^0.2.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-tap-event-plugin": "^3.0.2",
    "react-toolbox": "^2.0.0-beta.12",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "universal-cookie": "^2.1.2",
    "yarn": "^0.22.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "offline-plugin": "^4.6.1",
    "pm2": "^2.4.0",
    "raw-loader": "^0.5.1",
    "sw-precache-webpack-plugin": "^0.9.0"
  }
}
